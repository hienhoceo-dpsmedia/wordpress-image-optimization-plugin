# Error messages and additional strings
msgid "Server does not support %s format conversion."
msgstr "Máy chủ không hỗ trợ chuyển đổi định dạng %s."

msgid "Neither Imagick nor GD format support is available on this server."
msgstr "Không có hỗ trợ định dạng Imagick hoặc GD trên máy chủ này."

msgid "Invalid language selection"
msgstr "Lựa chọn ngôn ngữ không hợp lệ"

